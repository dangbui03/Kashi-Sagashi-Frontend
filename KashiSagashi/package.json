  "type": "module",
  "scripts": {
    "dev": "concurrently --kill-others \"vite\" \"sass ./src/scss/style.scss ./src/index.css -w\"",
    "build": "concurrently --kill-others \"vite build\" \"sass ./src/scss/style.scss ./src/index.css\"",
